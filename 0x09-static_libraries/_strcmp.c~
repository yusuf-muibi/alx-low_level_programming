#include "main.h"
/**
* _strcmp - this is the Function to compare two strings
* @s1: this is the first string
* @s2: this is the second string
* Return: this returns the difference
*/
int _strcmp(char *s1, char *s2)
{
int i = 0;
while (s1[i] == s2[i])
{
if (s1[i] == '\0')
{
return 0;
}
i++;
}
 return (s1[i] - s2[i]);
}
